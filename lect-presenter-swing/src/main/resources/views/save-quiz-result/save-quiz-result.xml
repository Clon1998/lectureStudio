<?xml version="1.0" encoding="UTF-8"?>
<Panel name="save.quiz.result.title">
	<Panel layout="GridBagLayout" border="EmptyBorder(0, 20, 20, 20)">
		<CheckBox id="csvCheckBox" text="save.quiz.result.csv.format" border="EmptyBorder(0, 0, 10, 0)">
			<gridbagconstraints gridx="0" gridy="0" gridwidth="2" fill="GridBagConstraints.HORIZONTAL" />
		</CheckBox>
		<CheckBox id="pdfCheckBox" text="save.quiz.result.pdf.format" border="EmptyBorder(0, 0, 10, 0)">
			<gridbagconstraints gridx="0" gridy="1" gridwidth="2" fill="GridBagConstraints.HORIZONTAL" />
		</CheckBox>
		<Label text="save.quiz.result.location" border="EmptyBorder(10, 0, 5, 0)">
			<gridbagconstraints gridx="0" gridy="2" gridwidth="2" fill="GridBagConstraints.BOTH" />
		</Label>
		<TextField id="pathTextField">
			<gridbagconstraints gridx="0" gridy="3" weightx="1" fill="GridBagConstraints.HORIZONTAL" anchor="GridBagConstraints.WEST" />
		</TextField>
		<Button id="selectPathButton" text="button.browse">
			<gridbagconstraints gridx="1" gridy="3" anchor="GridBagConstraints.WEST" insets="0,5,0,0" />
		</Button>
		<HBox gap="5" border="EmptyBorder(10, 0, 0, 0)">
			<gridbagconstraints gridx="0" gridy="4" gridwidth="2" weighty="1" fill="GridBagConstraints.HORIZONTAL" anchor="GridBagConstraints.SOUTHEAST" />
			<box.hglue />
			<Button id="closeButton" text="button.close" />
			<Button id="saveButton" text="button.save" enabled="false" />
		</HBox>
	</Panel>
</Panel>