<?xml version="1.0" encoding="UTF-8"?>
<Panel name="save.documents.title">
	<Panel layout="GridBagLayout" border="EmptyBorder(0, 20, 20, 20)">
		<TitledSeparator text="save.documents.select.individual" border="EmptyBorder(0, 0, 10, 0)">
			<gridbagconstraints gridx="0" gridy="0" gridwidth="2" fill="GridBagConstraints.HORIZONTAL" />
		</TitledSeparator>
		<VBox id="individualContainer" gap="5">
			<gridbagconstraints gridx="0" gridy="1" gridwidth="2" anchor="GridBagConstraints.WEST" fill="GridBagConstraints.HORIZONTAL" />
		</VBox>
		<TitledSeparator text="save.documents.select.merge" border="EmptyBorder(30, 0, 5, 0)">
			<gridbagconstraints gridx="0" gridy="2" gridwidth="2" fill="GridBagConstraints.BOTH" />
		</TitledSeparator>
		<TextField id="pathTextField">
			<gridbagconstraints gridx="0" gridy="3" weightx="1" fill="GridBagConstraints.HORIZONTAL" anchor="GridBagConstraints.WEST" insets="5,0,15,0" />
		</TextField>
		<Button id="selectPathButton" text="button.browse">
			<gridbagconstraints gridx="1" gridy="3" anchor="GridBagConstraints.WEST" insets="5,5,15,0" />
		</Button>
		<HBox gap="5" border="EmptyBorder(10, 0, 0, 0)">
			<gridbagconstraints gridx="0" gridy="4" gridwidth="2" weighty="1" fill="GridBagConstraints.HORIZONTAL" anchor="GridBagConstraints.SOUTHEAST" />
			<box.hglue />
			<Button id="closeButton" text="button.close" />
			<Button id="saveMergedButton" text="save.documents.save.merged" enabled="false" />
		</HBox>
	</Panel>
</Panel>